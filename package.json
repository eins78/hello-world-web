{
  "name": "hello-world-web",
  "version": "0.0.0-notversionedhere.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "bin": {
    "hello-world-web": "./bin/www.js",
    "healthcheck": "./bin/healthcheck.js"
  },
  "files": [
    "app.js",
    "config.js",
    "bin",
    "lib",
    "public",
    "routes",
    "views",
    "README.md",
    "LICENSE",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "start": "DEBUG='hello-world-web:*' NODE_ENV=production node ./bin/www",
    "dev": "npm-run-all --parallel dev:serve dev:watch",
    "dev:serve": "DEBUG='hello-world-web:*' nodemon -d 1.0 -e html,js,css,json,env -w '**/*' -w .env -w ./bin/www* ./bin/www",
    "dev:watch": "pnpm -F hello-world-web-lit-ssr-demo run watch",
    "dev-trace-sync-io": "export NODE_OPTIONS='--trace-sync-io' && $npm_execpath run dev:serve",
    "build": "pnpm -F hello-world-web-lit-ssr-demo i && pnpm -F hello-world-web-lit-ssr-demo run build",
    "postinstall": "pnpm run build",
    "lint": "eslint",
    "cy": "cypress open",
    "test": "$npm_execpath run lint && $npm_execpath run test:types && echo 'Lint and Typecheck successful!' && echo 'No tests yet!'",
    "test:types": "tsc"
  },
  "dependencies": {
    "@lit-labs/ssr": "^3.2.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "lit-html": "^3.1.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.1.1",
    "@testing-library/cypress": "^10.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "cypress": "^13.13.1",
    "eslint": "^9.1.1",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "globals": "^15.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "packageManager": "pnpm@9.5.0+sha256.dbdf5961c32909fb030595a9daa1dae720162e658609a8f92f2fa99835510ca5"
}
